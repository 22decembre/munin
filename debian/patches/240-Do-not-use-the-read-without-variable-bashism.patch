 plugins/node.d.linux/selinux_avcstat.in |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/plugins/node.d.linux/selinux_avcstat.in b/plugins/node.d.linux/selinux_avcstat.in
